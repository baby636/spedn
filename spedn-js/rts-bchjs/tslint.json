{
    "extends": "tslint:recommended",
    "rules": {
        "max-line-length": {
            "options": [
                120
            ]
        },
        "arrow-parens": false,
        "no-bitwise": false,
        "member-access": false,
        "interface-name": false,
        "trailing-comma": false,
        "curly": false,
        "max-classes-per-file": false,
        "no-console": false,
        "object-literal-sort-keys": false
    },
    "jsRules": {
        "max-line-length": {
            "options": [
                120
            ]
        }
    }
}